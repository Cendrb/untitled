<!DOCTYPE html>
<html>
<head>
  <title><%= get_title %></title>
  <%= stylesheet_link_tag 'application', 'data-turbolinks-track' => true, media: 'all' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <%= action_cable_meta_tag %>
  <meta name="viewport" content="width=device-width, initial-scale=0.5">
</head>
<body id="material_body_html">
<div id="material_container">
  <div id="material_header">
    <div id="material_header_content">
      <a href="<%= root_path %>">
        <svg id="material_header_logo" width="96" height="54" xmlns="http://www.w3.org/2000/svg">
          <g>
            <text xml:space="preserve" text-anchor="start" font-family="Helvetica, Arial, sans-serif" font-size="39" id="svg_21" y="40.5" x="6" fill-opacity="null" stroke-opacity="null" stroke-width="2" stroke="#ffffff" fill="#ffffff">Kana</text>
          </g>
        </svg>
      </a>
      <ul id="material_nav_menu">
        <li><%= link_to 'PÅ™ehled', root_path %></li>
        <li><%= link_to 'Disker', disk_controller_diskform_path %></li>
        <li><%= link_to 'Cyka', dota_heroes_path %></li>
        <li><%= link_to 'Administrace', summary_administration_path %></li>
      </ul>
      <div id="material_header_info">

      </div>
    </div>
  </div>
  <div id="material_body">
    <div id="material_body_content">
      <% if notice %>
          <p id="notice">
            <%= notice.html_safe %>
          </p>
      <% end %>
      <% if alert %>
          <p id="alert">
            <%= alert.html_safe %>
          </p>
      <% end %>
      <%= yield %>
    </div>
  </div>
  <div id="material_footer">
    <div id="material_footer_content">
      <div id="material_footer_left">

      </div>
      <div id="material_footer_right">

      </div>
    </div>
  </div>
</div>
</body>
</html>
